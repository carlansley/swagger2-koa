{
  "name": "swagger2-koa",
  "version": "3.0.1",
  "description": "Koa 2 middleware for loading, parsing and validating requests via swagger2",
  "main": "dist/index.js",
  "engines": {
    "node": ">=14"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carlansley/swagger2-koa.git"
  },
  "keywords": [
    "swagger",
    "swagger2",
    "typescript",
    "koa",
    "koa2"
  ],
  "author": "Carl Ansley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/carlansley/swagger2-koa/issues"
  },
  "homepage": "https://github.com/carlansley/swagger2-koa#readme",
  "typings": "./dist/index.d.ts",
  "prettier": "@checkdigit/prettier-config",
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@koa/router": "^10.1.1",
    "@types/koa-bodyparser": "^5.0.1",
    "debug": "^4.3.3",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-send": "^5.0.1",
    "swagger-ui-dist": "^3.23.11",
    "swagger2": "^3.0.1"
  },
  "devDependencies": {
    "@checkdigit/eslint-config": "^6.0.0",
    "@checkdigit/prettier-config": "^1.0.2",
    "@checkdigit/typescript-config": "^2.2.0",
    "@types/debug": "4.1.7",
    "@types/jest": "^27.4.0",
    "@types/koa": "^2.13.4",
    "@types/koa__cors": "^3.1.1",
    "@types/koa__router": "^8.0.11",
    "@types/koa-send": "^4.1.3",
    "@types/node": "^17.0.12",
    "@types/supertest": "^2.0.11",
    "coveralls": "^3.1.1",
    "eslint": "^8.7.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "maintainers": [
    {
      "email": "carl.ansley@gmail.com",
      "name": "Carl Ansley"
    }
  ],
  "jest": {
    "collectCoverage": true,
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "<rootDir>/build",
      "<rootDir>/coverage",
      "<rootDir>/node_modules"
    ]
  },
  "scripts": {
    "preversion": "npm test",
    "version": "npm run dist && git add -A dist",
    "postversion": "git push && git push --tags",
    "build": "rm -rf build && tsc && cp src/schema.json build",
    "dist": "rm -rf dist && tsc src/index.ts -m commonjs --outDir dist --sourcemap --target es2020 --lib es2020 -d --pretty --noImplicitAny --esModuleInterop --resolveJsonModule",
    "clean": "rm -rf build && rm -rf coverage && rm -rf node_modules",
    "lint": "eslint -f unix src/**/*.ts",
    "lint:fix": "eslint -f unix src/**/*.ts --fix",
    "prettier": "prettier --list-different src/**/*.ts",
    "prettier:fix": "prettier --write src/**/*.ts",
    "test": "tsc --noEmit && jest && npm run lint && npm run prettier",
    "cover:browser": "open coverage/lcov-report/index.html",
    "coveralls": "coveralls < coverage/lcov.info"
  }
}
